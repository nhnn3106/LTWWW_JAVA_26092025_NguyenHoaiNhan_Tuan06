<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd">
    <bean id="employee1" class="fit.iuh.se.bai02.beans.xmlbased.Employee">
        <constructor-arg name="id" value="001"/>
        <constructor-arg name="name" value="Nguyễn Văn Tèo"/>
        <constructor-arg name="address" ref="address1"/>
    </bean>
    <bean id="address1" class="fit.iuh.se.bai02.beans.xmlbased.Address">
        <property name="city" value="HCM"/>
        <property name="country" value="VN"/>
        <property name="state" value="VN"/>
    </bean>

</beans>